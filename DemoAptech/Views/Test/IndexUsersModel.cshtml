@model IEnumerable<DemoAptech.Models.User>
@{
    ViewBag.Title = "IndexUsersModel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html>
<body>
    <h3>User Details</h3>  @{
        var user = Model;
    }
    @foreach (var u in user)
    {
        @u.name <br /> @u.address<br /> @u.email<br />
        <br />
    }
</body>
</html>

